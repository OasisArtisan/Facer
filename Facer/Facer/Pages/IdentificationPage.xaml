<?xml version="1.0" encoding="utf-8" ?>
<facer_pages:CustomContentPage
        xmlns:facer_pages="clr-namespace:Facer.Pages"
        xmlns="http://xamarin.com/schemas/2014/forms"
        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
        x:Class="Facer.Pages.IdentificationPage">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="AcceptButton" Text="Accept"/>
            <Button x:Name="RetakeButton" Text="Retake"/>
            <Button x:Name="CancelButton" Text="Cancel"/>
        </StackLayout>
    </ContentPage.Content>
</facer_pages:CustomContentPage>